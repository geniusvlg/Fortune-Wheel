@font-face {
    font-family: "Roboto Regular";
    src: url("../fonts/Roboto-Regular.ttf");
}

@font-face {
    font-family: "Roboto Light";
    src: url("../fonts/Roboto-Light.ttf");
}

body {
    font-family: "Roboto Light";
}

header {
    background-color:#070909;
}

footer {
    background-color:#070909;
}

.navbar {
    margin-bottom:0px;
}

.navbar-brand {
    float:none;
    height:auto;
    padding:0px;
}

.navbar-brand > img {
    display:normal !important;
}

.navbar-nav > li {
    padding:0px 10px 0px 10px;
}

.navbar-nav > li > a {
    color:white;
    text-transform:uppercase;
    -webkit-transition: all .5s;
    -o-transition: all .5s;
    transition: all .5s;
}

.nav li a:focus, .nav li a:hover {
    color: orange;
    background-color: none !important;
}

.section1 {
    padding-top:100px;
    background : -moz-linear-gradient(0% 50% 0deg,rgba(209, 211, 212, 1) 0%,rgba(241, 242, 242, 1) 100%);
    background : -webkit-linear-gradient(0deg, rgba(209, 211, 212, 1) 0%, rgba(241, 242, 242, 1) 100%);
    background : -webkit-gradient(linear,0% 50% ,100% 50% ,color-stop(0,rgba(209, 211, 212, 1) ),color-stop(1,rgba(241, 242, 242, 1) ));
    background : -o-linear-gradient(0deg, rgba(209, 211, 212, 1) 0%, rgba(241, 242, 242, 1) 100%);
    background : -ms-linear-gradient(0deg, rgba(209, 211, 212, 1) 0%, rgba(241, 242, 242, 1) 100%);
    -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#D1D3D4', endColorstr='#F1F2F2' ,GradientType=0)";
    background : linear-gradient(90deg, rgba(209, 211, 212, 1) 0%, rgba(241, 242, 242, 1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#D1D3D4',endColorstr='#F1F2F2' , GradientType=1);
}

.section1 .bg_image {
    background-image: url('../images/image_01.png');
    background-repeat: no-repeat;
    background-size: contain;
    background-position: 90% center;
}

.section1 .text {
    font-size : 45px;
    text-transform : uppercase;
    color : #157C71;
    padding:35px 0px 100px 0px;
    letter-spacing:0px;
}

.section1 .text1 {    
    font-weight:bold;
}

.section1 .text1 span {
    font-family: "Roboto Regular";
    font-weight:bolder;
}

.section1 .text2 {
    color:#d68531;
}

.section1 .app_button {
    float:left;
    padding:0px 0px 20px 0px;
}

.section1 .app_button img {
    width:45%;
}

.section1 .qr_code {
    position:absolute;
    top:60px;
    right:0px;
}

